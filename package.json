{
  "name": "smart-home",
  "displayName": "Smart Home",
  "description": "One keypress to toggle between start of line and start of text",
  "version": "0.0.1",
  "publisher": "AlexanderRodseth",
  "engines": {
    "vscode": "^1.5.x"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "activationEvents": [
    "onCommand:extension.smartHome.toggleCursorHome"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.smartHome.toggleCursorHome",
        "title": "Toggle between moving the cursor to the start of the line and the start of the text"
      }
    ]
  },
  "devDependencies": {
    "vscode": "^0.11.13"
  },
  "bugs": {
    "url": "https://github.com/xyproto/vscode-smart-home/issues/"
  },
  "homepage": "https://github.com/xyproto/vscode-smart-home/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/xyproto/vscode-smart-home/"
  }
}
